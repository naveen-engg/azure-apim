{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "projectName": {
      "value": "myproject"
    },
    "environment": {
      "value": "staging"
    },
    "location": {
      "value": "eastus"
    },
    "vnetAddressPrefix": {
      "value": "10.5.0.0/16"
    },
    "apimSubnetPrefix": {
      "value": "10.5.1.0/24"
    },
    "appGatewaySubnetPrefix": {
      "value": "10.5.2.0/24"
    },
    "apimSku": {
      "value": "Developer"
    },
    "apimCapacity": {
      "value": 1
    },
    "appGatewayCapacity": {
      "value": 2
    },
    "appGatewayMaxCapacity": {
      "value": 7
    },
    "auth0Domain": {
      "value": "contoso-staging.auth0.com"
    },
    "auth0Audience": {
      "value": "https://api-staging.contoso.com"
    },
    "mulesoftBackendUrl": {
      "value": "https://api-staging.mulesoft.contoso.com"
    },
    "adminObjectId": {
      "value": "YOUR-AZURE-AD-OBJECT-ID"
    },
    "logRetentionDays": {
      "value": 60
    },
    "enableWafPreventionMode": {
      "value": true
    }
  }
}
