{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "projectName": {
      "value": "myproject"
    },
    "environment": {
      "value": "prod"
    },
    "location": {
      "value": "eastus"
    },
    "vnetAddressPrefix": {
      "value": "10.10.0.0/16"
    },
    "apimSubnetPrefix": {
      "value": "10.10.1.0/24"
    },
    "appGatewaySubnetPrefix": {
      "value": "10.10.2.0/24"
    },
    "privateEndpointSubnetPrefix": {
      "value": "10.10.3.0/24"
    },
    "apimSku": {
      "value": "Premium"
    },
    "apimCapacity": {
      "value": 2
    },
    "appGatewayCapacity": {
      "value": 3
    },
    "appGatewayMaxCapacity": {
      "value": 10
    },
    "auth0Domain": {
      "value": "contoso.auth0.com"
    },
    "auth0Audience": {
      "value": "https://api.contoso.com"
    },
    "mulesoftBackendUrl": {
      "value": "https://api.mulesoft.contoso.com"
    },
    "privateDnsZoneName": {
      "value": "privatelink.azurewebsites.net"
    },
    "adminObjectId": {
      "value": "YOUR-AZURE-AD-OBJECT-ID"
    },
    "logRetentionDays": {
      "value": 90
    },
    "enableWafPreventionMode": {
      "value": true
    }
  }
}
